function get_user_region(){const user_timezone=Intl.DateTimeFormat().resolvedOptions().timeZone;const timezone_map={"Asia/Kolkata":"Mumbai","Asia/Calcutta":"Mumbai","Asia/Riyadh":"KSA","Asia/Aden":"KSA","Asia/Dubai":"UAE","Asia/Muscat":"UAE","Europe/London":"London","GB":"London","Europe/Zurich":"Zurich","Europe/Berlin":"Zurich","Asia/Singapore":"Singapore","Asia/Brunei":"Singapore","Asia/Bahrain":"Bahrain","Asia/Qatar":"Bahrain","Africa/Johannesburg":"Cape Town","Africa/Windhoek":"Cape Town","America/New_York":"Virginia","America/Detroit":"Virginia","Africa/Cairo":"Egypt"};return timezone_map[user_timezone]||user_timezone;}
window.addEventListener("DOMContentLoaded",()=>{const user_region=get_user_region();let pack_type="ROW";if(["KSA","UAE","Qatar","Bahrain","Oman","Kuwait","Egypt","Cape Town"].includes(user_region)){pack_type="mena";}else if(user_region==="Mumbai"){pack_type="ind";}
const packNoPack=pack_type==="ROW"?"pack":"nopack";const packElems=document.getElementsByClassName(packNoPack);while(packElems.length>0){packElems[0].parentNode.removeChild(packElems[0]);}
if(packNoPack==="nopack"){const pricingTableRows=document.querySelectorAll("tr");Array.from(pricingTableRows).filter((x)=>(x.className!==''&&x.className!==pack_type))[0].remove();}});